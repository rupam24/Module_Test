
#Pods written in Swift can only be integrated as frameworks; add `use_frameworks!`
platform :ios, '10.0'

target 'Project_New' do
use_frameworks!
inhibit_all_warnings!

pod 'Alamofire'
pod 'SnapKit', '~> 4.0.0'
pod 'AlamofireImage'
 

 target 'Project_NewTests' do
    inherit! :search_paths
 end

end

